        "Regex": "^.{2,256}$",
        "Description": "SignUp LastName"
      },
      "EmailValidator": {
        "Regex": "^[a-zA-Z0-9\\+\\.\\_\\%\\-]{1,256}\\@[a-zA-Z0-9][a-zA-Z0-9\\-]{0,64}(\\.[a-zA-Z0-9][a-zA-Z0-9\\-]{0,25})$",
