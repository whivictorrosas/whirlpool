		"ErrorDescription": "Please enter a valid password"
      },
      "LocationNameValidator": {
        "Regex": "^.{1,256}$",
        "Description": "SignUp Location Name"
      },
      "Address1Validator": {
        "Regex": "^.{2,256}$",
        "Description": "SignUp AddressLine1"
      },
      "CityValidator": {
        "Regex": "^.{2,256}$",
        "Description": "SignUp City"
      },
      "ZipCodeValidator": {
        "Regex": "^(\\d{5}(-\\d{4})?|[A-CEGHJ-NPRSTVXY]\\d[A-CEGHJ-NPRSTV-Z] ?\\d[A-CEGHJ-NPRSTV-Z]\\d)$",
        "Description": "SignUp Zipcode",
		"ErrorDescription": "Please enter a valid zipcode"
      },
      "MobilePhoneValidator": {
        "Regex": "^\\d{10}$",
        "Description": "Signup Mobile Phone"
      },
      "SAIDValidator": {
        "Regex": "^\\bWPR[a-zA-Z0-9]{10}|[a-zA-Z0-9]{13}$",
        "Description": "Scan to Connect SAID",
		"ErrorDescription": "The codes entered are invalid.Please check the QR code sticker for the codes and try again."
      },
      "MACIDValidator": {
        "Regex": "^[a-fA-F0-9]{12}$",
        "Description": "Scan to Connect MAC ID",
		"ErrorDescription": "The codes entered are invalid.Please check the QR code sticker for the codes and try again."
      },
      "InviteFirstNameValidator": {
        "Regex": "^.{1,256}$",
        "Description": "Invite User First Name"
      },
      "InviteLastNameValidator": {
        "Regex": "^.{1,256}$",
        "Description": "Invite User Last Name"
      },
      "InviteEmailValidator": {
        "Regex": "^[a-zA-Z0-9\\+\\.\\_\\%\\-]{1,256}\\@[a-zA-Z0-9][a-zA-Z0-9\\-]{0,64}(\\.[a-zA-Z0-9][a-zA-Z0-9\\-]{0,25})$",
        "Description": "Invite User Email",
		"ErrorDescription": "Please enter a valid password"
      },
      "ResetPasswordValidator": {
        "Regex": "(?=^.{8,60}$)(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?!.*\\s).*$",
